<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd">

<aplicacion nombre="AgenciaAduanera"> 
	
	<!--
	Un módulo por defecto se asume para cada
	controlador en <modulo-defecto/> 
	-->
	<modulo-defecto>		  
		<controlador nombre="Typical"/>		
	</modulo-defecto>
	
	<!--
	Podemos definir módulos adicionales, por ejemplo: 	
	<modulo nombre="ProfesorSoloLectura">
		<modelo nombre="Profesor"/>
		<controlador nombre="Print"/>
	</modulo>		
	-->

    <modulo nombre="Expediente">
        <modelo nombre="Expediente"/>
        <controlador nombre="ExpedienteCustom"/>
        <controlador nombre ="Typical"/>
    </modulo>

    <modulo nombre="Declaracion">
        <modelo nombre="Declaracion"/>
        <controlador nombre="DeclaracionCustom"/>
        <controlador nombre="Typical"/>
    </modulo>



</aplicacion>

